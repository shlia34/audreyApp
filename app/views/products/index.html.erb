<div class = "items">

<% @products.each do |product| %>

  <div class = "item">
    <div class = "item__image">
      <img class = "activator" src = <%= product.image_url %>>
    </div>
    <div class = "item__bottom">
      <div class = "item__bottom__title">
        <a><%= product.title %></a>
      </div>
      <div class = "item__bottom__time">
        <a><%= product.date%></a>
      </div>
    </div>
  </div>
<% end %>

<%= paginate @products %>

</div>
